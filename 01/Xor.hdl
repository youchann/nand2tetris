/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in= a, out= nota);
    Not(in= b, out= notb);
    Or(a= a, b= notb, out= c);
    Or(a= nota, b= b, out= d);
    Nand(a= c, b= d, out= out);
}